{
  "schema": "https://turborepo.org/schema.json",
  "globalEnv": [],
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build#exh-nt": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_STUDIO_URL",
        "NEXT_PUBLIC_SANITY_DATASET",
        "NEXT_PUBLIC_SANITY_PROJECT_ID"
      ]
    },
    "build#exh-nt-studio": {
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "dev#exh-nt": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_STUDIO_URL",
        "NEXT_PUBLIC_STUDIO_URL",
        "NEXT_PUBLIC_SANITY_DATASET",
        "NEXT_PUBLIC_SANITY_PROJECT_ID"
      ]
    },
    "deploy": {
      "cache": false,
      "dependsOn": []
    },
    "clean": {
      "cache": false
    }
  }
}